export
function d $f(l %x, ...) {
@start
	%vp =l alloc8 32
	vastart %vp
	%i =l vaarg %vp
	%n =d vaarg %vp
	ret %n
}

export
function w $g(l %fmt, ...) {
@start
	%vp =l alloc8 32
	vastart %vp
	%r =w call $print(l %fmt, l %vp)
	ret %r
}

